{
  "diffEditor.ignoreTrimWhitespace": false,

  "editor.fontSize": 13,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80],

  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "explorer.openEditors.visible": 0,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.classpath": true,
    "**/.idea": true,
    "**/.settings": true,
    "**/bin/**": true,
    "**/bower_components/**": true,
    "**/build/**": true,
    "**/node_modules/**": true,
    "**/target/**": true,
    "**/target-eclipse/**": true
  },
  "search.exclude": {
    "**/.classpath": true,
    "**/.idea": true,
    "**/.settings": true,
    "**/bin": true,
    "**/bower_components": true,
    "**/build": true,
    "**/node_modules": true,
    "**/target": true,
    "**/target-eclipse": true
  },

  "terminal.integrated.fontFamily": "",
  "terminal.integrated.fontSize": 13,

  "workbench.colorTheme": "Monokai",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.colorCustomizations": {
    "terminal.background": "#1D1D1D",
    "terminal.foreground": "#F7F7F7",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#C75646",
    "terminal.ansiGreen": "#8EB33B",
    "terminal.ansiYellow": "#c4a000",
    "terminal.ansiBlue": "#72B3CC",
    "terminal.ansiMagenta": "#C8A0D1",
    "terminal.ansiCyan": "#218693",
    "terminal.ansiWhite": "#B0B0B0",
    "terminal.ansiBrightBlack": "#5D5D5D",
    "terminal.ansiBrightRed": "#E09690",
    "terminal.ansiBrightGreen": "#CDEE69",
    "terminal.ansiBrightYellow": "#FFE377",
    "terminal.ansiBrightBlue": "#9CD9F0",
    "terminal.ansiBrightMagenta": "#FBB1F9",
    "terminal.ansiBrightCyan": "#77DFD8",
    "terminal.ansiBrightWhite": "#F7F7F7"
  },

  "eslint.run": "onSave",
  "eslint.packageManager": "yarn",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "vue",
      "autoFix": true
    }
  ],

  "workbench.panel.location": "bottom",
  "java.jdt.ls.vmargs": "-noverify -Xmx4G -XX:+UseG1GC -XX:+UseStringDeduplication",
  "java.referencesCodeLens.enabled": true,
  "java.implementationsCodeLens.enabled": true,

  "vetur.format.defaultFormatter.html": "js-beautify-html",
  "vetur.format.defaultFormatterOptions": {
    "js-beautify-html": {
      "wrap_attributes": "force-expand-multiline"
    }
  },
  "eslint.autoFixOnSave": true,

  "shellformat.flag": "-i 2",
  "window.zoomLevel": 0
}
